function [[%tabstop:example]]_menu($may_cache) {
  $items = array();

  if ($may_cache) {
    $items[] = array(
      'path' =&gt; 'admin/settings/[[%tabstop:example]]',
      'title' =&gt; t('[[%tabstop:Example]]'),
      'callback' =&gt; 'drupal_get_form',
      'callback arguments' =&gt; array('[[%tabstop:example]]_admin_form'),
      'access' =&gt; user_access('administer site configuration'),
    );
  }

  return $items;
}


/**
 * Menu callback - Admin settings form.
 */
function [[%tabstop:example]]_admin_form() {
  $form = array();
  return system_settings_form($form);
}


